    <element name="Martial Prodigy" type="Racial Trait" source="Tasha's Cauldron of Everything" id="ID_WOTC_TCOE_RACIAL_TRAIT_GITHYANKI_MARTIAL_PRODIGY">
        <description>
            <p>You are proficient with light and medium armor and with shortswords, longswords, and greatswords.</p>
        </description>
        <sheet display="false">
            <description>You are proficient with light and medium armor and with shortswords, longswords, and greatswords.</description>
        </sheet>
        <rules>
            <grant type="Proficiency" id="ID_PROFICIENCY_ARMOR_PROFICIENCY_LIGHT_ARMOR" />
            <grant type="Proficiency" id="ID_PROFICIENCY_ARMOR_PROFICIENCY_MEDIUM_ARMOR" />
            <select type="Proficiency" name="Custom Weapon Proficiencies" supports="Custom Tools" />
            <select type="Proficiency" name="Custom Weapon Proficiencies" supports="Custom Weapons" number="1" />
            <select type="Proficiency" name="Custom Weapon Proficiencies" supports="Custom Weapons" number="1" />
        </rules>
    </element>
